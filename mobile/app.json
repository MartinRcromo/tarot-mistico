{
  "expo": {
    "name": "Tarot Místico",
    "slug": "tarot-mistico",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1024.png",
    "scheme": "tarot-mistico",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tarotmistico.app",
      "icon": "./assets/images/icon-1024.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#d4af37"
      },
      "package": "com.tarotmistico.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-1071804711955925~9582390558"
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "Tarot Místico usa identificadores para mostrarte anuncios relevantes"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
